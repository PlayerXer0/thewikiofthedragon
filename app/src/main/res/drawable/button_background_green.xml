<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="#00FF00"/> <!-- Color verde para el estado presionado -->
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#009900"/> <!-- Color verde oscuro para el estado normal -->
        </shape>
    </item>
</selector>
